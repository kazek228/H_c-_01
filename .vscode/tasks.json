{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"build",
				"ConsoleApp.csproj"
			],
			"group": "build",
			"problemMatcher": [
				"$msCompile"
			],
			"isBackground": false
		},
		{
			"label": "run",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"run",
				"--project",
				"ConsoleApp.csproj"
			],
			"group": "test",
			"problemMatcher": [],
			"isBackground": false,
			"dependsOn": "build"
		}
	]
}